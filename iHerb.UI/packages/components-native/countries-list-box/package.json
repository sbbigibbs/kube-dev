{
  "name": "@iherb/ui-countries-list-box",
  "version": "1.3.24",
  "main": "lib/",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "prepublish": "npm run clean && npm run compile && npm run build",
    "build": "iherb-ui-tools build --modules -o ./lib -s ./src/index.js --src src --library-target commonjs --output-name UiCountriesListBox --platforms android",
    "compile": "babel src -d lib",
    "clean": "rimraf ./lib",
    "release": "npm run clean && npm run compile && npm run build && npm publish",
    "start": "iherb-ui-tools start -o ./dist -s ./examples/web/index.js --public ./examples/web/public --html ./examples/web/public/index.html --src examples/web,src --module-resolver-alias-name @iherb/ui-countries-list-box --module-resolver-alias-val ./src --platforms android",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@iherb/ui-flag-sprite": "^1.3.24"
  },
  "description": "Primitive React countries list box component across targets",
  "author": {
    "email": "jonathan.nacionales@iherb.net",
    "name": "Jonathan Nacionales",
    "url": "https://gitlab.iherb.net/u/jonathan.nacionales"
  },
  "keywords": [
    "ui-countries-list-box"
  ],
  "engines": {
    "node": ">= ^8.0.0",
    "npm": ">= ^5.0.0"
  },
  "devDependencies": {
    "@iherb/ui-tools-cli": "^1.3.24",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
