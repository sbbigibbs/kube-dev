{
  "name": "@iherb/ui-component-promo-code",
  "version": "1.3.18",
  "main": "lib/",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "prepublish": "npm run clean && npm run compile && npm run build",
    "build": "iherb-ui-tools build --modules -o ./lib -s ./src/index.js --src src --library-target commonjs --output-name UIComponentShippingMethod --platforms android,web",
    "compile": "babel src -d lib --source-maps --ignore **/__tests__",
    "clean": "rimraf ./lib",
    "release": "npm run clean && npm run compile && npm run build && npm publish",
    "start": "iherb-ui-tools start -o ./dist -s ./examples/web/index.js --public ./examples/web/public --html ./examples/web/public/index.html --src examples/web,src --module-resolver-alias-name @iherb/ui-component-shipping-method --module-resolver-alias-val ./src --platforms web",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@iherb/react-primitive-text-input": "^1.2.4",
    "@iherb/ui-translations-cart": "^1.3.15"
  },
  "description": "Primitive React shipping method ui component across targets",
  "author": {
    "email": "jonathan.nacionales@iherb.net",
    "name": "Jonathan Nacionales",
    "url": "https://gitlab.iherb.net/u/jonathan.nacionales"
  },
  "keywords": [
    "ui-component-shipping-method"
  ],
  "engines": {
    "node": ">= ^8.0.0",
    "npm": ">= ^5.0.0"
  },
  "devDependencies": {
    "@iherb/ui-tools-cli": "^1.3.14",
    "rimraf": "^2.6.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0",
      "stage-1"
    ]
  }
}
