{
  "ecmaVersion": 7,
  "loadEagerly": [
    "src/**/*.js",
  ],
  "dontLoad": [
    "node_modules/**",
    "lib/**",
    "build/**",
  ],
  "libs": [
    "browser",
    "ecmascript",
    "chai"
  ],
  "plugins": {
    "node": {},
    "node_resolve": {},
    "modules": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "es_modules": {},
    "complete_strings": {}
  }
}
